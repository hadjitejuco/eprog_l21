#include <stdio.h>
struct student{
	int studno;
	char name[50];
	float grade;
};
int main(){
	student s;
	printf ("Enter student number, name and grade ");
	scanf ("%d%s%f",&s.studno,&s.name, &s.grade);
	printf ("\nStudent numbers %d",s.studno);
	printf ("\nStudent Name %s",s.name);
	printf ("\nStudent Grade %f",s.grade);
	return 0;
}
