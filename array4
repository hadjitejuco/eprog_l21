
#include <stdio.h>

int main()
{
    int a[100]; //int array 
    int i,n;  //loop
    printf ("Enter n:");
    scanf ("%d",&n);
    printf ("Please enter %d numbers\n", n);
    for (i=0; i<n;i++){
        scanf ("%d",&a[i]);
    }
    printf ("ODD NUMBERS\n");
    for(i=0;i<n;i++){
        if(a[i]%2==1)
        printf("%3d",a[i]);
    }
    printf ("\nEVEN NUMBERS\n");
    for(i=0;i<n;i++){
        if(a[i]%2==0)
        printf("%3d",a[i]);
    }
    return 0;
}
