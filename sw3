//sw3
/*
	range 			grade
	90 - 100		A	90 91 92 93 94 95 96 97 98 99 100
	80-89			B   80 81 82 83 84 85 86 87 88 89 90
	70-79			C   70 71 72 73 74 75 76 77 78 79 80
	60-69			D
	below 60 		F
*/
#include <stdio.h>
int main()
{
	int g;
	printf ("Enter Grade ");
	scanf ("%d",&g);
	switch (g){
		case  90 :  case  91 : case  92: case 93:
		case  94 :  case  95 : case  96: case 97:
		case  98 :  case 99 :  case  100 : printf ("A");break;
		case  80 :  case  81 : case  82: case 83:
		case  84 :  case  85 : case  86: case 87:
		case  88 :  case 89 :   printf ("B");break;
		default : printf ("others "); break;
	}



	return 0;
}
