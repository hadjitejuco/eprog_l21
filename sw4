//sw3
/*
	range 			grade
	90 - 100		A	90 91 92 93 94 95 96 97 98 99 100
	80-89			B   80 81 82 83 84 85 86 87 88 89 90
	70-79			C   70 71 72 73 74 75 76 77 78 79 80
	60-69			D
	below 60 		F
*/
#include <stdio.h>
int main()
{
	int g;
	printf ("Enter Grade ");
	scanf ("%d",&g);
	switch (g){
		case 90  ...  100: printf ("A"); break;
		case 80  ...  89: printf ("B"); break;
		case 70  ...  79: printf ("C"); break;
		case 60  ...  69: printf ("D"); break;
		case 50  ...  59: printf ("E"); break;
		default : printf ("others "); break;
	}
	return 0;
}
