//if13
/*
	Year			Level
	1				Freshmen
	2				Sophomore
	3				Junior
	4				Senior
	
	> others
*/
#include <stdio.h>
int main(){
	char yr;
	printf ("Enter year ");
	scanf ("%c",&yr);
	if (yr=='1'){
		printf ("Freshmen");
	}else if(yr=='2'){
		printf ("Sophomore");
	}else if(yr=='3'){
		printf ("Junior");
	}else if(yr=='4'){
		printf ("Senior");
	}else{
		printf ("others");
	}
	return 0;
}
