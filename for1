//count numbers -> Enter n: 10 -> 1 .. 10
#include <stdio.h>
int main(){
    int i, n; //i -> loop, n->input 
    printf ("Enter N: ");
    scanf ("%d",&n);    //10
    //   (1)  (2)  (4)
    for (i=1; i<=n;i++)   //true
    {
        printf ("%3d",i);//(3)
    }
    return 0;
}
   /*n      i    i<=n       printf      i++
     10     1    1<=10-t    1           2 
            2    2<=10-t    2           3 
            3    -t         3           4
            4    -t         4           5
            ....
            9    -t         9           10
            10   10<=10-t   10          11
            11   11<=10-f
    */

