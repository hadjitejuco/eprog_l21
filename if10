//if10
/*
	range 			grade
	90 - above		A
	80-89			B
	70-79			C
	60-69			D
	below 60 		F
*/
#include <stdio.h>
int main()
{
	int g;
	printf ("Enter Grade ");
	scanf ("%d",&g);
	if (g>=90){
		printf ("A");
	}else if (g>=80){
		printf ("B");
	}else if (g>=70){
		printf ("C");
	}else if (g>=60){
		printf ("D");
	}else{
		printf ("E");
	}
	return 0;
}
