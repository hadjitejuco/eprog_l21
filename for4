#include <stdio.h>
int main(){
    int i, n; //i -> loop, n->input 
    printf ("Enter n ");//10
    scanf ("%d",&n);    
    //   (1)  (2)  (4)
    for (i=1; i<=10;i++)   //true
    {
            printf ("%3d",i*n);//(3)
    }
    return 0;
}
