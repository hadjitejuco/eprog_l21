
#include <stdio.h>

int main()
{
    int a[100]; //int array 
    int i,n;  //loop
    printf ("Enter n:");
    scanf ("%d",&n);
    printf ("Please enter %d numbers\n", n);
    for (i=0; i<n;i++){
        scanf ("%d",&a[i]);
    }
    printf ("\nEntered numbers are\n ");
    for (i=0;i<n;i++){
        printf ("%3d",a[i]);
    }
    return 0;
}
